{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "ecmaFeatures": {

  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "arrow-body-style": [2, "as-needed"],
    "arrow-parens": [2, "as-needed"],
    "arrow-spacing": 2,
    "no-console": "off",
    "constructor-super": 2,
    "generator-star-spacing": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-restricted-imports": 0,
    "no-this-before-super": 2,
    "no-var": 2,
    "object-shorthand": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-reflect": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "require-yield": 2,
    "block-scoped-var": 2,
    "global-require": 2,
    "no-unused-vars": [
      "error",
      { "args": "none" }
    ]
  }
}
